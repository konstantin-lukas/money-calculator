{
  "name": "moneydew",
  "version": "0.3.0",
  "description": "This is a library for formatting currencies designed for frontend applications. Also has arithmetic capabilities. Arithmetic implementation is slow and not designed for large-scale backend applications.",
  "main": "index.js",
  "module": "index.mjs",
  "typings": "index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "test-all": "jest --coverage",
    "test-money": "jest money.test.ts --coverage  --collectCoverageFrom=src/money.ts",
    "test-money-calculator": "jest money_calculator.test.ts --coverage  --collectCoverageFrom=src/money_calculator.ts",
    "test-money-formatter": "jest money_formatter.test.ts --coverage  --collectCoverageFrom=src/money_formatter.ts",
    "generate-documentation": "typedoc",
    "server": "http-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konstantin-lukas/moneydew.git"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/konstantin-lukas/moneydew/issues"
  },
  "files": [
    "./dist",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "homepage": "https://github.com/konstantin-lukas/moneydew#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "esbuild": "^0.19.2",
    "http-server": "^14.1.1",
    "jest": "^29.4.3",
    "rollup": "^3.28.1",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.6.2",
    "typedoc": "^0.23.28",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^11.1.3"
  }
}
